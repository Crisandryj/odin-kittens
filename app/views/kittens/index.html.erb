<%= render "flash" %>
<% @kittens.each do |kitten| %>
<div>
  <li><%=  "Name: #{kitten.name}" %></li>
  <p> <%= "Age: #{kitten.age}" %></p>
  <p><%= "Softness: #{kitten.softness}" %></p>
  <p><%= "Cuteness: #{kitten.cuteness}" %></p>

  <p><%= button_to "Delete", kitten, method: :delete %></p>
</div>

<% end %>
